{
  "name": "school-sis-seed-scripts",
  "version": "1.0.0",
  "description": "Database seeding scripts for School SIS with secure password generation",
  "main": "generate_secure_passwords.js",
  "scripts": {
    "generate-passwords": "node generate_secure_passwords.js",
    "generate-passwords:dev": "node generate_secure_passwords.js development",
    "generate-passwords:test": "node generate_secure_passwords.js testing",
    "generate-passwords:staging": "node generate_secure_passwords.js staging",
    "generate-passwords:prod": "node generate_secure_passwords.js production",
    "install-deps": "npm install",
    "clean": "rm -rf generated/*.json generated/*.sql generated/.env.*",
    "validate": "node validate_seed_data.js"
